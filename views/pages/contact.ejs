<head>
     <%- include('../partials/head'); -%>
</head>

<header>
   <%- include('../partials/header'); -%>
</header>

<%- include('../partials/navigation'); -%>

<h2 class="questions"><%= topicHead %></h2>

<!-- views/contact.ejs -->
<div class="form-header">
   <h3></h3></br>
</div>


<form method="post" action="/contactsubmit" novalidate>


   <div class="form-row">
    <div class="form-group col-md-3">
      <label for= "fname">First Name</label>
      <input type="text" class="form-control txtbox" placeholder="First name" id="fname" name="fname" size="10" >
    </div>
    <div class="form-group col-md-3">
      <label for= "lname">Last Name</label>
      <input type="text" class="form-control txtbox" placeholder="Last name" id="lname" name="lname">
    </div>
  </div>
  <div class="form-row">
  <div class="form-group col-md-6">
    <label for="email">Email</label>
    <input class="form-control txtbox" id="email" name="email" type="email" placeholder="name@example.com" size="10"/>
  </div>
 </div>
  <div class="form-row">
  <div class="form-group col-md-6">
    <label for="msg">Message or Question</label>
    <textarea class="form-control txtbox" id="msg" name="msg" rows="3" autofocus></textarea>
  </div>
    </div>

   
  <div class="form-actions">
    <button class="btn btn-sm btn-outline-primary txtbox" type="submit">Send</button>
  </div>

</form>


<footer>
    <%- include('../partials/footer'); -%>
</footer>